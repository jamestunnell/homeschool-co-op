<%= simple_form_for @event do |f| %>
  <%= f.input :title %>
  <%= f.input :description %>
  <%= f.input :start_time %>
  <%= f.input :end_time %>
  <%= f.association :building %>
  <%= f.hidden_field(:photo_cache) %>
  <%= f.input :photo, as: :file %>
  <p><%= f.button :submit, :class => 'btn-primary' %></p>
<% end %>