<%= simple_form_for(@term) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :season, collection: Term.seasons.keys, include_blank: false %>
    <%= f.input :workshop %>
    <%= f.input :start_date %>
    <%= f.input :end_date %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, :class => 'btn btn-primary' %>
  </div>
<% end %>
